/* ==UserStyle==
@name         GitLab Dark by Mew
@namespace    gitlab.com/vednoc/gitlab-dark
@version      0.2.0
@description  Dark theme for GitLab.
@author       vednoc
@homepageURL  https://gitlab.com/vednoc/dark-gitlab
@supportURL   https://gitlab.com/vednoc/dark-gitlab/issues
@preprocessor stylus

@var text     max_width "Maximum width for fluid layout" 100%
@var checkbox p_header  "Center project header elements" 0
@var checkbox dark_logo "Switch to our darker logo" 0
@var checkbox bg_invert "Background inversion" 1
@var text     bg_image  "Background url/base64" "url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA8AAAAPCAYAAAA71pVKAAAABHNCSVQICAgIfAhkiAAAAAlwSFlzAAAOxAAADsQBlSsOGwAAABl0RVh0U29mdHdhcmUAd3d3Lmlua3NjYXBlLm9yZ5vuPBoAAAA7SURBVCiRY2BgYHBgQABbBgaGlwwMDE4ExBwYGBheMRBQRJQY/TUyQN3uQIZGBzhBhsbRACNR44gMMADUtz9VRtuY9AAAAABJRU5ErkJggg==")"
@var text     bg_filter "Background filters" opacity(0.05)
@var text     bg_size   "Background size" 15px
@var text     bg_repeat "Background repeat" repeat

@var color dark    "Dark color."    #2e2e2e
@var color darker  "Darker color."  #212121
@var color light   "Light color."   #d1d1d1
@var color lighter "Lighter color." #e9e9e9
@var color icon    "Icon color."    #e1e1e1
@var color accent  "Accent color."  #7289da
@var color yellow  "Yellow color."  #f0c674
@var color orange  "Orange color."  #de935f
@var color red     "Red color."     #a54242
@var color magenta "Magenta color." #b294bb
@var color violet  "Violet color."  #85678f
@var color blue    "Blue color."    #5f819d
@var color cyan    "Cyan color."    #5e8d87
@var color green   "Green color."   #8c9440
@var color shadow  "Shadow"         rgba(0,0,0,0.07)

==/UserStyle== */

@-moz-document regexp("https?:\/\/(.*)?(gitlab.com|gitgud.io|salsa.debian.org|framagit.org|gitlab.gnome.org)(\/.*)?") {
  :root {
    --dark      : dark;
    --darker    : darker;
    --light     : light;
    --lighter   : lighter;
    --accent    : accent;
    --icon      : icon;
    --shadow    : shadow;
    --box-shadow: 0 2px 3px 0 var(--shadow);
    --yellow    : yellow;
    --orange    : orange;
    --red       : red;
    --magenta   : magenta;
    --violet    : violet;
    --blue      : blue;
    --cyan      : cyan;
    --green     : green;
    --width     : max_width;
    --background: bg_image
  }

  ::-webkit-scrollbar {
    width: 12px;
  }
  ::-webkit-scrollbar-track {
    background: var(--dark);
  }
  ::-webkit-scrollbar-thumb {
    border: 2px solid var(--dark);
    background: var(--darker);
  }

  h1, h2, h3, h4, h5, h6, p {
    color: var(--light) !important;
  }
  a {
    transition: 200ms ease-in-out;
    color: var(--accent);
  }
  a:hover,
  a:focus {
    color: var(--accent);
  }

  code {
    border: 1px solid var(--darker);
    background-color: var(--dark);
  }
  table td {
    background-color: var(--dark);
  }

  /* Button reset. */
  .btn,
  .btn:hover,
  .btn:focus,
  .btn.btn-default {
    transition: 200ms ease-in-out !important;
    color: var(--light);
    border-color: var(--darker);
    background-color: var(--dark);
  }
  .btn:active,
  .btn.active {
    color: var(--light) !important;
    border-color: var(--accent) !important;
    background-color: var(--dark) !important;
  }
  /* Green buttons. */
  .btn.btn-success,
  .btn.btn-new,
  .btn.btn-create,
  .btn.comment-btn,
  .btn.btn-save,
  .btn.commit-btn.js-commit-button.btn-create {
    color: var(--lighter) !important;
    border-color: var(--green) !important;
    background-color: var(--green) !important;
    box-shadow: var(--box-shadow) !important;
  }
  /* Blue buttons. */
  .btn.btn-info,
  .btn.btn-primary,
  .btn.btn-register,
  .project-feature-toggle.is-checked {
    color: var(--lighter) !important;
    border-color: var(--blue) !important;
    background-color: var(--blue) !important;
    box-shadow: var(--box-shadow) !important;
  }
  /* Close buttons. */
  .btn.btn-close,
  .btn.btn-close-color,
  .btn.btn-cancel {
    color: var(--lighter) !important;
    border-color: var(--red) !important;
    background-color: var(--red) !important;
    box-shadow: var(--box-shadow) !important;
  }
  /* Remove buttons. */
  .btn.btn-danger,
  .btn.btn-danger:hover,
  .btn.btn-danger:focus,
  .btn.btn-remove,
  .btn.btn-remove:hover,
  .btn.btn-remove:focus,
  .btn.btn-red,
  .btn.btn-red:hover,
  .btn.btn-red:focus {
    color: var(--lighter) !important;
    border-color: var(--red) !important;
    background-color: var(--red) !important;
    box-shadow: var(--box-shadow) !important;
  }
  /* Inverted buttons. */
  .btn.btn-inverted.btn-remove {
    color: var(--lighter);
    border-color: var(--red);
    background-color: var(--red);
    box-shadow: var(--box-shadow) !important;
  }
  /* Blank buttons. */
  .btn-blank {
    background: transparent !important;
  }
  /* Disabled buttons. */
  .btn.disabled,
  .btn.disabled:hover,
  .btn[disabled],
  .btn[disabled]:hover {
    color: var(--lighter) !important;
    border-color: var(--darker) !important;
    background-color: var(--darker) !important;
    opacity: 0.6 !important;
    box-shadow: var(--box-shadow) !important;
  }
  /* On event. */
  .btn:hover:not([disabled="disabled"]),
  .btn:focus:not([disabled="disabled"]) {
    transform: translateY(-2px);
  }

  /* Global styles. */
  [class*="author-link"] {
    color: var(--accent) !important;
  }

  /* Sign in page. */
  body,
  body.navless {
    color: var(--light) !important;
    background-color: var(--dark) !important;
  }
  /* Navbar. */
  .navbar-empty {
    border-bottom-color: var(--darker);
    background-color: var(--darker);
  }

  /* Sign in/register buttons. */
  .login-page .new-session-tabs {
    border-color: var(--darker);
    box-shadow: 0 0 0 1px var(--darker);
  }
  .login-page .new-session-tabs li {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--darker) !important;
  }
  .login-page .new-session-tabs li a.active,
  .login-page .new-session-tabs li a:hover {
    color: var(--lighter) !important;
    border-color: var(--accent) !important;
    background-color: var(--darker) !important;
  }

  /* Sign in/register section. */
  .login-page .login-box,
  .login-page .omniauth-container {
    box-shadow: 0 0 0 1px var(--darker);
  }
  .login-page .form-group input {
    color: var(--lighter) !important;
    border-color: var(--darker) !important;
    background: var(--dark) !important;
  }
  /* Misc text color. */
  .light,
  .description-block {
    color: var(--light) !important;
  }

  /* Footer. */
  hr.footer-fixed {
    border-top-color: var(--darker);
    background: none !important;
  }
  .footer-container {
    background: none !important;
  }
  .footer-container a {
    color: var(--accent);
  }


  /* Navbar. */
  .navbar {
    background: var(--darker) !important;
    border-color: var(--darker) !important;
	box-shadow: var(--box-shadow) !important;
  }
  /* Logo text color. */
  .navbar .logo-text svg {
    fill: var(--lighter) !important }
  /* Search. */
  .navbar .search form {
    color: var(--lighter) !important;
    background-color: var(--dark) !important;
    background-color: var(--dark) !important; }
  .navbar #search {
    color: var(--lighter) !important;
  }
  /* Collapsable nav. */
  .navbar ul li.active a,
  .navbar-collapse ul > li:hover a {
    color: var(--light) !important;
    background-color: var(--darker) !important;
  }
  /* Input placeholder. */
  .navbar input::placeholder {
    color: var(--light) !important;
  }


  /* Dropdowns. */
  .dropdown-menu,
  .dropdown-menu-nav {
    box-shadow: var(--box-shadow) !important;
    border-color: var(--darker);
    background-color: var(--darker);
  }
  /* Loading. */
  [class*="dropdown-loading"] {
    background-color: var(--dark);
  }
  .fa-spinner:before {
    color: var(--light);
  }
  /* Title. */
  .dropdown-title {
    color: var(--lighter);
    border-bottom-color: var(--darker);
  }
  /* Dropdown toggle. */
  .dropdown-toggle,
  .dropdown-menu-toggle {
    color: var(--light);
    border-color: var(--darker) !important;
    background-color: var(--dark);
  }
  /* Dropdown search. */
  .dropdown-input-field,
  .default-dropdown-input {
    border-color: var(--darker);
    background-color: var(--dark);
  }
  /* Frequently visited. */
  [class*="frequent-items"] .section-header {
    color: var(--light) !important;
  }
  [class*="frequent-items"] .section-empty,
  [class*="frequent-items"] [class*="namespace"]  {
    color: var(--lighter) !important;
  }
  /* On event. */
  .dropdown-input-field:focus,
  .default-dropdown-input:focus {
    border-color: var(--blue);
    box-shadow: 0 0 1px 3px var(--shadow);
  }
  /* Separator. */
  .divider,
  .dropdown-divider,
  .dropdown-menu .divider,
  .dropdown-menu [class*="dropdown-sidebar"] {
    border-color: var(--darker);
  }
  /* Items. */
  .dropdown-menu li a,
  .dropdown-menu li button,
  .dropdown-menu li .menu-item,
  .dropdown-menu .current-user,
  .navbar-sub-nav {
    color: var(--light) !important;
  }
  .dropdown-header {
	color: var(--lighter) !important;
  }
  /* On event. */
  .dropdown-menu li button:hover,
  .dropdown-menu li a:not(.disable-hover):hover,
  .dropdown-menu li a:focus,
  .dropdown-menu li a.is-focused,
  .dropdown-menu .droplab-item-selected > .menu-item,
  .dropdown-menu > li:hover > .menu-item {
    color: var(--light);
    background-color: var(--shadow) !important;
  }
  .dropdown.show > a > *, .dropdown.show > button > *, .show > button, .show > button > *, .show > #notifications-button,
  .show > #clone-dropdown > *,
  #nav-projects-dropdown > button[aria-expanded="true"], #nav-groups-dropdown > button[aria-expanded="true"] {
	transition: none !important;
    transition-delay: none !important;
	color: var(--accent) !important;
	fill: var(--accent) !important;
  }
  /* Open dropdowns. */
  .navbar-sub-nav > li.active > button,
  .navbar-sub-nav > li > button:hover,
  .navbar-sub-nav > li > a:hover,
  .dropdown.show > button,
  .dropdown.show > a {
    color: var(--lighter) !important;
    background-color: var(--darker) !important;
  }
  /* Text color. */
  .dropdown-menu-inner-content,
  .dropdown-toggle-text.is-default {
    color: var(--light);
  }
  .dropdown-menu-inner-title,
  .dropdown-menu .form-group label {
    color: var(--lighter);
  }
  /* Icon colors. */
  .nav a svg,
  .nav .dropdown.show a:hover svg {
    fill: var(--icon) !important;
  }


  /* A brand new dropdown? */
  .select2-drop,
  .select2-drop.select2-drop-above {
    color: var(--light);
    border-color: var(--darker);
    background-color: var(--darker);
  }
  /* Flashing on input. */
  .select2-container-multi .select2-choices .select2-search-field input.select2-active {
    background: var(--dark) !important;
  }
  /* Input/select elements. */
  .select2-choice,
  .select2-drop input,
  .select2-container-multi .select2-choices {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }
  /* On event. */
  .select2-results .select2-highlighted {
    color: var(--lighter);
  }
  .select2-results .select2-highlighted .select2-result-label {
    background-color: var(--darker);
  }
  .select2-results .select2-highlighted .select2-result-label:hover {
	  background-color: var(--shadow) !important;
  }
  .select2-arrow {
    padding-left: 10px;
    background-color: var(--dark) !important;
  }


  /* New project. */
  .gitlab-tabs {
    overflow: hidden;
    border-radius: 4px 4px 0 0;
    border-color: var(--darker) !important;
    background-color: var(--darker) !important;
  }
  .gitlab-tabs li:not(:last-child) {
    border-right-color: var(--darker);
  }
  .gitlab-tabs .nav-link.active,
  .gitlab-tabs .nav-link:hover {
    border-radius: 0;
  }
  /* On event. */
  .gitlab-tabs li a.active,
  .gitlab-tabs li a:hover {
    color: var(--lighter) !important;
    border-color: var(--accent) !important;
    background-color: var(--dark) !important;
  }

  /* Tab content. */
  .gitlab-tab-content {
    border-radius: 0 0 4px 4px;
    border-color: var(--darker);
    background-color: var(--dark);
  }
  /* Inputs. */
  .input-group .input-group-btn {
    background: transparent !important;
  }
  .gitlab-tab-content [class*="input-group"] {
    color: var(--lighter) !important;
    border-color: var(--darker) !important;
    background-color: transparent !important;
  }
  .gitlab-tab-content .template-option {
    border-color: var(--darker) !important;
  }
  /* Text colors. */
  .gitlab-tab-content label,
  .option-title {
    color: var(--lighter) !important;
  }
  .option-description {
    color: var(--light) !important;
  }
  .account-btn-link,
  .profile-settings-sidebar a,
  .settings-sidebar a {
    color: var(--accent);
  }
  /* Form control. */
  .form-control {
    transition: border-color 200ms ease-in-out !important;
    color: var(--lighter) !important;
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }
  /* On event. */
  .form-control:hover,
  .form-control:focus {
    border-color: var(--blue) !important;
    box-shadow: 0 0 2px 1px var(--shadow);
  }


  /* User Settings :: Everything. */
  .settings-message {
    color: var(--light) !important;
    border-color: var(--accent) !important;
    background-color: var(--dark) !important;
  }
  /* Social icons. */
  .provider-btn-group {
    border-color: var(--darker);
    background: var(--darker) !important;
  }
  .provider-btn-image {
    border-color: var(--darker);
  }
  .provider-btn {
    color: var(--accent) !important;
    background-color: var(--darker);
  }
  /* Path. */
  .input-group-text {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--darker) !important;
  }
  /* Linked emails. */
  .account-well {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }
  /* List items. */
  ul.bordered-list > li {
    border-bottom-color: var(--darker);
  }
  /* Active sessions. */
  .card .list-group-item {
    color: var(--light) !important;
    border: 1px solid var(--darker) !important;
    background-color: var(--dark) !important;
  }
  /* Pipeline quota table. */
  .pipeline-quota,
  .table tr td {
    border-color: var(--darker) !important;
  }
  .pipeline-quota .progress {
    box-shadow: 0 0 0 1px var(--shadow);
    background-color: var(--darker);
  }
  /* Text color. */
  .text-muted,
  .description {
    color: var(--light) !important;
  }
  .profile-settings-content a {
    color: var(--accent);
  }
  .form-control::placeholder {
    color: var(--light) !important;
  }


  /* Projects dashboard. */
  .breadcrumbs .breadcrumbs-container {
    border-color: var(--darker) !important;
  }
  .breadcrumbs,
  .breadcrumb.repo-breadcrumb a,
  .breadcrumbs-list > li > a,
  .breadcrumbs-sub-title a {
    color: var(--lighter);
  }

  /* Nav section. TODO: Fix the fixed layout version. */
  .container-fluid > .top-area {
    margin-top: 10px;
    padding: 0 10px;
    border-radius: 4px;
    border: 1px solid var(--darker);
    background-color: var(--dark);
  }
  /* Wrapper. */
  .top-area,
  .nav-links,
  .wiki-page-header {
    border-bottom-color: var(--darker);
  }
  .nav-links li a,
  .nav-links li a .badge.badge-pill {
    color: var(--light) !important;
  }
  .nav-links li.active a,
  .nav-links li:hover a,
  .nav-links li.active a .badge.badge-pill {
    color: var(--lighter) !important;
    border-bottom-color: var(--accent) !important;
  }


  /* Billing. */
  .bg-info {
    background-color: var(--darker) !important;
  }
  .billing-plans .card .card-header, .billing-plans .card-slim .card-header {
    background-color: var(--accent) !important;
  }
  .billing-plans .card.current .card-body, .billing-plans .current.card-slim .card-body,
  .billing-plans .card, .billing-plans .card-slim {
    border-color: var(--accent) !important;
    background-color: var(--shadow) !important;
  }
  /* Items. */
  .billing-plans .card .card-body .feature-list li,
  .billing-plans .card-slim .card-body .feature-list li {
    background-color: var(--shadow) !important;
  }
  /* Price per month. */
  .billing-plans .card.current .card-body .price-per-month,
  .billing-plans .current.card-slim .card-body .price-per-month,
  .billing-plans .card.current .card-body .price-per-year,
  .billing-plans .current.card-slim .card-body .price-per-year,
  .billing-plans .card .card-body .price-per-month,
  .billing-plans .card-slim .card-body .price-per-month {
    color: var(--accent) !important;
  }
  /* Annually. */
  .billing-plans .card .card-body .price-per-year, .billing-plans .card-slim .card-body .price-per-year {
    color: var(--green) !important;
  }

  /* Applications. */
  .text-secondary {
    color: var(--light) !important;
  }

  /* Emails. */
  .email-badge .badge-verification-status.verified:hover:not(span),
  .email-badge .badge-verification-status.verified:not(span) {
    background-color: var(--green) !important;
    border-color: var(--green) !important;
    color: var(--lighter) !important;
  }

  /* SSH Keys. */
  pre,
  code.key-fingerprint {
    background-color: var(--darker) !important;
    border-color: var(--darker) !important;
    color: var(--lighter) !important;
  }


  // * Projects list.
  .projects-list {
    margin: 10px 0;
    border-radius: 4px;
    border: 1px solid var(--darker);
    background-color: var(--dark);
    // Items.
    > li {
      padding: 5px;
      transition: 200ms ease-in-out;
      border-bottom-color: var(--darker);
      // On event.
      &:hover {
        background-color: var(--shadow);
      }
    }
  }

  /* Avatar background. */
  .avatar,
  .avatar-circle,
  .avatar-container,
  .breadcrumbs-links .avatar-tile,
  .header-user-avatar {
    border: none !important;
    background-color: transparent !important;
  }
  .avatar-container,
  .identicon {
    color: var(--light);
  }
  img.avatar {
    filter: drop-shadow(1px 1px 2px var(--shadow));
  }

  /* Project details. */
  .project-details .text-plain,
  .project-stats .nav .stat-link {
    color: var(--light);
  }
  .project-details .text-plain:hover,
  .project-stats .nav .stat-link:hover {
    color: var(--lighter);
  }
  .user-access-role {
    color: var(--light);
    border-color: var(--darker);
    background-color: var(--darker);
  }
  /* Empty project. */
  .project-empty-note-panel {
    border-color: var(--darker);
  }
  .git-empty pre.bg-light {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }

  /* Project :: Commits. */
  .commit-header {
    border-radius: 4px;
    border: 1px solid var(--darker) !important;
    border-color: var(--darker);
    background-color: var(--darker);
  }
  .commit-list .commit {
    border-color: var(--darker);
  }
  /* Commit expand. */
  .commit .text-expander {
    color: var(--accent);
    border-color: var(--darker);
    background-color: var(--dark);
  }
  /* Commit description. */
  .commit .commit-row-description {
    color: var(--light);
    border-left-color: var(--accent);
  }
  .commit-list .commit a {
    color: var(--light);
  }

  /* Project :: Branches. */
  .content-list li {
    border-color: var(--darker) !important;
  }
  /* Graph color. */
  .divergence-graph .graph-separator,
  .divergence-graph .graph-side [class*="bar"] {
    background-color: var(--accent);
  }
  /* Text color. */
  .commit-row-message,
  .branch-commit,
  .content-list li [class*="item"],
  .content-list li [class*="graph"] [class*="count"] {
    color: var(--light) !important;
  }
  .branch-commit .commit-sha,
  .branch-commit .gfm-commit,
  .branch-commit .gfm-commit_range {
    color: var(--accent) !important;
  }

  /* Show commit message. */
  .text-expander {
    color: var(--light);
    border-color: var(--darker);
    background: var(--darker);
  }
  .text-expander:hover {
    color: var(--accent);
    border-color: var(--darker);
    background: var(--darker);
  }


  /* Project :: Contributors. */
  #contributors-master .tint-box,
  #contributors .person .spark {
    color: var(--light);
    background: var(--darker);
  }
  #contributors-master .tint-box .area {
    fill: var(--green);
  }
  #contributors .person .area-contributor {
    fill: var(--orange);
  }
  /* Lines. */
  #contributors-master .tint-box .axis .domain,
  #contributors-master .tint-box .axis line,
  #contributors .spark .axis .domain,
  #contributors .spark .axis line {
    stroke: var(--dark) !important;
  }
  /* Text color. */
  #contributors-master .tint-box .axis text,
  #contributors .axis text {
    fill: var(--light);
  }


  /* Project :: Graph. */
  .project-network {
    margin-top: 10px;
    border-radius: 4px;
    border-color: var(--darker);
  }
  .project-network .controls {
    border: var(--darker);
    background-color: var(--darker);
  }
  /* Graph background. */
  .project-network .network-graph {
    fill: var(--light);
    background-color: var(--dark);
  }
  /* Hover over commit. */
  .project-network .network-graph > svg rect[width*="310"] {
    stroke: var(--shadow);
    fill: var(--darker);
  }
  /* Text color. */
  .project-network .network-graph > svg text {
    fill: var(--light);
  }
  /* Sidebar. */
  .project-network .network-graph > svg rect[width*="40"],
  .project-network .network-graph > svg rect[width*="30"] {
    stroke-width: 0;
    stroke: var(--darker);
    fill: var(--darker);
  }
  /* TODO: Style extras in the future. */


  /* Modal. */
  .modal .modal-content {
    background-color: var(--dark) !important;
  }
  .modal .modal-header {
    border-color: var(--darker);
    background-color: var(--darker);
  }
  .modal .modal-body {
    border-radius: 0 0 4px 4px;
    background-color: var(--dark) !important;
  }

  /* Branch info border color. */
  .commit-box,
  .info-well .well-segment:not(:last-of-type) {
    border-color: var(--darker);
  }
  /* Commit description. */
  .commit-description,
  .commit-row-description {
    color: var(--light) !important;
    border-left-color: var(--accent) !important;
  }
  /* Link. */
  .commit .gfm {
    color: var(--accent) !important;
  }
  /* Deleted branch. */
  .info-well .limit-box {
    background-color: var(--red) !important;
  }
  ul.content-list > li,
  .info-well > .well-segment {
    padding: 8px;
  }
  /* Center avatar. */
  [id*="commit"] a > img,
  .info-well > .well-segment a > img {
    margin-top: 3px;
    margin-left: 3px;
  }
  /* Adjust sha. */
  .commit > .commit-detail,
  .info-well > .well-segment .commit-detail {
    align-items: center;
  }

  /* Invalid GPG button. */
  .gpg-status-box.invalid,
  .gpg-status-box.invalid:focus {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }
  .gpg-popover-user-link {
    color: var(--light);
  }
  /* Popover. */
  .popover {
    border-color: var(--darker);
    background-color: var(--dark);
  }
  .popover-header {
    border-bottom-color: var(--darker) !important;
    background-color: var(--shadow) !important;
  }
  .popover-body {
    color: var(--light) !important;
  }
  /* Arrow. */
  .popover .arrow::after {
    border-top-color: var(--dark) !important;
  }
  .popover .arrow::before {
    border-top-color: var(--darker) !important;
  }

  /* Recently pushed branches. */
  .container-fluid > .row-content-block.white {
    margin-top: 10px;
    padding: 8px;
    border-radius: 4px;
    border: 1px solid var(--darker);
    background-color: var(--dark);
  }

  /* Commits :: Diff summary. */
  .diff-files-changed {
    border-radius: 4px;
    padding: 4px;
    margin: 10px 0;
    border: 1px solid var(--darker);
    background-color: var(--dark);
  }
  .diff-files-changed > .files-changed-inner {
    display: flex;
    flex-flow: row-reverse wrap;
    /* justify-content: space-between; */
  }
  /* Center buttons vertically. */
  .diff-files-changed .inline-parallel-buttons {
    float: none;
    padding: 3px 5px 5px 5px;
  }
  /* TODO: Remove if they fix diff header && fix responsiveness. */
  .diff-files-changed .commit-stat-summary {
    margin: auto auto auto 0;
    padding: 0 8px;
    background-color: var(--transparent) !important;
  }
  /* Diff file header. */
  .diff-file .file-title:hover,
  .diff-file .file-title-flex-parent:hover,
  .diff-file .diff-content {
    background-color: var(--dark);
  }
  /* Text color. */
  .branch-info > span,
  .file-title-flex-parent a {
    color: var(--light);
  }
  /* Badge. */
  .info-well .badge.badge-gray {
    color: var(--light);
    border: 1px solid var(--darker);
    background-color: var(--dark);
  }


  /* Timeline content. */
  .timeline-entry {
    color: var(--light);
    border-color: var(--darker);
    background-color: transparent;
  }
  /* Comments. */
  .notes .timeline-entry-inner {
    border-color: var(--darker) !important;
  }
  .note-headline-light,
  .discussion-headline-light,
  .note-headline-meta .system-note-separator {
    color: var(--light);
  }
  /* Text color. */
  ul.notes>li .note-body .note-text blockquote p, ul.notes>li .note-body .note-text .blockquote p {
    color: var(--light) !important;
  }
  .notes a,
  .notes a:hover {
    color: var(--accent) !important;
  }

  /* Issues :: Details - add comment. */
  .common-note-form .md-area {
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }
  /* On focus. */
  .common-note-form .md-area.is-focused {
    outline: none !important;
    box-shadow: 0 0 2px 4px var(--shadow) !important;
  }
  /* Input area. */
  .markdown-area {
    color: var(--light);
    border-color: var(--darker);
    background-color: var(--dark);
  }
  /* Toolbar colors. */
  .edited-text,
  .comment-toolbar,
  .nav-links,
  .md {
    color: var(--light);
    border-color: var(--darker) !important;
  }
  /* Markdown selectors. */
  .md h1,
  .md h2 {
    border-color: var(--darker) !important;
  }
  .md hr {
    border-color: var(--darker);
  }
  .md pre {
    border-color: var(--darker);
  }
  .md blockquote,
  .blockquote {
    border-left-color: var(--accent) !important;
    background-color: var(--shadow);
  }
  /* Images. */
  .md img:not(.emoji) {
    border-color: var(--darker);
  }
  /* Attach a file. */
  .markdown-selector {
    color: var(--accent);
  }
  /* Fullscreen (zen) mode. */
  .zen-backdrop.fullscreen {
    background-color: var(--darker);
  }
  .zen-backdrop.fullscreen textarea {
    color: var(--light);
    background-color: var(--dark);
  }


  /* Left sidebar. */
  .nav-sidebar {
    border-right: 1px solid var(--darker) !important;
    background-color: var(--dark);
    box-shadow: none;
  }
  /* Hide scrollbar. */
  .nav-sidebar ::-webkit-scrollbar {
    display: none !important;
  }
  /* Items. */
  .nav-sidebar li a,
  .sidebar-sub-level-items a,
  .nav-sidebar .context-header > a {
    color: var(--light) !important;
  }
  .nav-sidebar li a:hover,  .nav-sidebar li a:focus,
  .sidebar-sub-level-items a:hover, .sidebar-sub-level-items a:focus {
    color: var(--lighter) !important;
    background-color: var(--shadow) !important;
  }

  /* Active sub-menu. */
  .nav-sidebar li.active {
    box-shadow: inset 4px 0 0 0 var(--accent) !important;
    background-color: var(--darker) !important;
  }
  /* Active tab. */
  .nav-sidebar .sidebar-sub-level-items .active a {
    box-shadow: inset -4px 0 0 0 var(--accent) !important;
    background-color: var(--dark);
  }
  /* Hide indicators. */
  @media (min-width: 576px) {
    .sidebar-sub-level-items > li.active {
      box-shadow: none !important;
      background-color: transparent !important;
    }
  }
  .nav-sidebar li.active .badge {
    color: var(--lighter) !important;
  }
  .nav-sidebar li.active svg,
  .toggle-sidebar-button svg {
    fill: var(--accent) !important;
  }

  /* Collapse button. */
  .nav-sidebar .toggle-sidebar-button {
    transition: 200ms ease-in-out !important;
    height: 44px;
  }
  .toggle-sidebar-button .collapse-text {
    color: var(--light) !important;
  }

  /* Dropdown. */
  .is-showing-fly-out .sidebar-sub-level-items {
    box-shadow: 2px 1px 3px var(--shadow) !important;
    border-radius: 0 4px 4px 0 !important;
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }
  /* Toggle button. */
  .toggle-sidebar-button,
  .close-nav-button {
    color: var(--light);
    border-color: var(--darker);
    background-color: var(--dark) !important;
  }


  /* Right sidebar. */
  .right-sidebar {
    border-left-color: var(--darker);
    background-color: var(--dark);
  }
  /* Separator. */
  .right-sidebar .block,
  .right-sidebar .gutter-toggle {
    border-bottom-color: var(--darker) !important;
  }
  /* Button background. */
  .right-sidebar .js-move-issue:hover,
  .right-sidebar.right-sidebar-collapsed .block:hover {
    background-color: var(--shadow) !important;
  }
  /* Text color. */
  .right-sidebar .title {
    color: var(--lighter);
  }
  .right-sidebar .no-value,
  .right-sidebar .sidebar-collapsed-icon {
    color: var(--light) !important;
  }
  .right-sidebar .btn-link,
  .right-sidebar .edit-link,
  .right-sidebar .lock-edit,
  .right-sidebar .confidential-edit {
    color: var(--lighter) !important;
  }
  /* Icons. */
  .right-sidebar .fa,
  .right-sidebar svg {
    color: var(--icon) !important;
    fill: var(--icon) !important;
  }


  /* Project/group page. */
  .project-home-panel,
  .group-home-panel {
    border-bottom-color: var(--darker);
  }
  /* Fix buttons. */
  .project-home-panel .count-badge-count,
  .project-home-panel .count-badge-button,
  .project-home-panel .btn,
  .project-home-panel input,
  .input-group-prepend .btn,
  .input-group-append .btn.btn-clipboard,
  .project-home-panel .dropdown-toggle,
  .project-home-panel .dropdown-menu-toggle {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--darker) !important;
  }
  /* Fix text color. */
  .project-home-panel .project-visibility,
  .project-home-panel .text-secondary,
  .project-home-panel .count-buttons .count {
    color: var(--light) !important;
  }
  /* Remove background from the license button. */
  .project-home-panel .project-metadata .project-license .btn {
    background: 0 !important;
  }
  /* Button group. */
  .input-group-prepend,
  .input-group-append {
    background-color: transparent !important;
  }
  /* Buttons. */
  .count-with-arrow .count,
  .clone-dropdown-btn,
  .btn-clipboard {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }
  /* Arrows. */
  .count-with-arrow .arrow::before {
    border-right-color: var(--darker) !important;
  }
  .count-with-arrow .arrow::after {
    border-right-color: var(--dark) !important;
  }
  /* Missing buttons. */
  .btn-missing {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--darker) !important;
  }
  /* Border colors. */
  .banner-callout.banner-non-empty-state,
  .project-stats {
    border-bottom-color: var(--darker);
  }
  /* Language bar. */
  .repository-languages-bar {
    background-color: transparent;
  }
  /* Languages. */
  .repository-languages-bar .progress-bar {
    margin: 0 4px 0 0;
    border-radius: 8px;
  }

  /* Project settings menu. */
  .settings {
    border-bottom-color: var(--darker);
  }
  .project-visibility-setting {
    border-radius: 4px 4px 0 0;
    border-color: var(--darker);
    background-color: var(--dark);
  }
  .project-feature-settings {
    border-radius: 0 0 4px 4px;
    border-color: var(--darker);
    background-color: var(--dark);
  }

  /* Latest commit message.. */
  .info-well {
    color: var(--light);
    border-color: var(--darker);
    background: var(--dark);
  }
  .info-well a {
    color: var(--lighter);
  }
  /* Commit button. */
  .commit-sha-group .label,
  .commit-sha-group .btn {
    color: var(--light);
    border-color: var(--darker);
    background-color: var(--dark);
  }


  /* Readme wrapper. */
  .file-holder {
    overflow: hidden;
    border-color: var(--darker) !important; }
  /* Readme header. */
  .tree-holder .file-title a:not(.btn) {
    color: var(--lighter);
  }
  /* Readme preview. */
  .file-holder .file-title-flex-parent,
  .file-holder .file-content {
    border-color: var(--darker);
    background: var(--dark);
  }

  /* Wiki. */
  .wiki {
    color: var(--light);
  }
  .wiki h1,
  .wiki h2 {
    color: var(--lighter) !important;
    border-bottom-color: var(--darker) !important;
  }
  .wiki blockquote,
  .wiki .blockquote {
    color: var(--lighter) !important;
    border-left-color: var(--accent) !important;
    background-color: var(--dark);
  }
  .wiki blockquote p,
  .wiki .blockquote p {
    color: var(--lighter) !important;
  }
  .wiki a {
    text-decoration: underline;
    color: var(--accent) !important;
  }
  .wiki pre,
  .wiki code {
    color: var(--lighter) !important;
    border-color: var(--darker) !important;
    background-color: var(--dark) !important;
  }
  .wiki > p > code {
    border: 1px solid var(--darker) !important;
  }
  .wiki img:not(.emoji) {
    border-color: var(--darker) !important;
  }
  .wiki table {
    color: var(--light);
  }
  .wiki table tr th {
    color: var(--lighter);
    border-bottom-color: var(--darker);
    background-color: var(--dark);
  }
  .wiki table th,
  .wiki table tr td {
    border-color: var(--darker);
  }


  /* Files. */
  .table-holder {
    border-radius: 4px;
    border: 1px solid var(--darker);
    background-color: var(--dark);
  }
  /* Remove unnecessary borders. */
  .tree-holder .tree-table tr {
    border-color: transparent !important;
  }
  .tree-holder .tree-table tr td {
    border-color: var(--darker) !important;
    transition: .2s ease;
  }
  /* Text color. */
  .tree-holder .tree-item .tree-item-file-name i,
  .tree-holder .tree-item .tree-item-file-name a,
  .tree-holder .tree-commit .tree-commit-link,
  .tree-holder .tree-time-ago {
    color: var(--light);
  }
  /* Background colors. */
  table.table tr th,
  .file-holder .file-title,
  .tree-holder .tree-table tr:hover:not(.tree-truncated-warning) td {
    border-color: var(--darker);
    background-color: var(--darker);
  }


  /* Profile cover. */
  .cover-block {
    border-color: var(--darker);
    background: var(--dark);
  }
  /* Fix master/history/find file/web ide section when low-resolution. */
  @media (max-width: 740px) {
    .nav-block {
      flex-direction: column;
    }
  }
  /* Scrolling tabs. */
  .scrolling-tabs-container {
    margin: 10px 0;
    padding: 0 10px;
    /* overflow: hidden; */
    border-radius: 4px;
    border: 1px solid var(--darker);
    background-color: var(--dark);
  }
  .scrolling-tabs-container .fade-right {
    background: linear-gradient(to left, var(--dark) 45%, transparent);
  }
  .scrolling-tabs-container .fade-left {
    background: linear-gradient(to right, var(--dark) 45%, transparent);
  }
  /* Profile activity. */
  #activity > .row-content-block {
    border-color: var(--darker) !important;
    background-color: transparent !important;
  }
  /* White activity rectangles. */
  #activity rect[fill*="#ededed"] {
    fill: var(--darker);
  }
  /* Nav. */
  .nav-links li a.active {
    color: var(--lighter) !important;
    border-color: var(--accent) !important;
  }

  /* Most recent activity. */
  #activity .event-item {
    color: var(--light);
    border-bottom-color: var(--darker);
  }
  /* Text color. */
  .cover-block .cover-desc,
  .nothing-here-block,
  .calendar-hint,
  .event-item .event-title,
  .content-list li {
    color: var(--light) !important;
  }
  /* Emphasized text color. */
  .cover-block .cover-title,
  .event-item a,
  .event-item .commit-sha{
    color: var(--lighter) !important;
  }
  /* Icons. */
  [class*="pushed-"] svg {
    fill: var(--icon) !important;
  }


  /* Dashboard. */
  .row-content-block {
    color: var(--light);
    border-color: var(--darker);
    background-color: var(--dark);
  }
  /* Sub-header found in compare branches. */
  .sub-header-block {
    border-color: var(--darker);
    background-color: transparent;
  }
  [class*="card"] > ul > li {
    transition: 200ms ease-in-out;
  }
  /* Text color. */
  .issuable-milestone,
  .issuable-info,
  .task-status,
  .issuable-updated-at {
    color: var(--light) !important;
  }
  /* Merged/closed. */
  .merge-request.merged,
  .issue.merged,
  .merge-request.closed,
  .issue.closed {
    border-color: var(--darker);
    background-color: var(--dark);
  }
  /* New issues. */
  .issue.today {
    border-color: var(--darker);
    background-color: var(--shadow);
  }
  /* Item on hover. */
  .card > ul > li:hover,
  .todos-list > .todo:hover {
    border-top-color: transparent !important;
    border-bottom-color: var(--darker) !important;
    background-color: var(--shadow) !important;
  }


