  .ci-status.ci-success {
    color: var(--green);
    border-color: var(--green);
  }
  .ci-status.ci-running {
    color: var(--blue);
    border-color: var(--blue);
  }
  .ci-status.ci-failed {
    color: var(--red);
    border-color: var(--red);
  }
  .ci-status.ci-created,
  .ci-status.ci-skipped,
  .ci-status.ci-manual {
    color: var(--lighter);
    border-color: var(--darker);
  }
  /* Badges */
  .badge-success {
    color: var(--light);
    background-color: var(--green);
  }

  /* Project CI/CD :: Pipeline details. */
  .pipeline-tab-content {
    background-color: transparent;
  }
  /* Pipeline node. */
  .pipeline-graph .build .build-content {
    border-width: 2px !important;
    color: var(--light);
    border-color: var(--darker);
    background-color: var(--dark);
  }
  /* On event. */
  .pipeline-graph .build .build-content:hover {
    color: var(--light) !important;
    border-color: var(--darker) !important;
    background-color: var(--shadow) !important;
  }
  /* Connectors. */
  .pipeline-graph .stage-column .curve::before,
  .pipeline-graph .stage-column .curve::after,
  .pipeline-graph .build::before,
  .pipeline-graph .build::after {
    border-color: var(--darker) !important;
  }

  /**
  * * Skeleton.
  * ? Changes color of loading elements.
  */
  .animation-container [class^="skeleton-line-"] {
    background-color: var(--darker);
  }
  .animation-container [class^="skeleton-line-"]::after {
    background-image: linear-gradient(to right, var(--darker) 0%, var(--dark) 100%);
  }


  // Invert illustrations.
  .banner-graphic > svg,
  img[src*="/assets/illustrations/"] {
    filter: opacity(0.9) invert(0.9) hue-rotate(40deg) drop-shadow(1px 1px 3px var(--shadow)) !important;
  }

  // * Centers project header elements; old design lookalike.
  if p_header {
    .project-home-panel > div {
      display: flex;
      flex-direction: column;
      > div {
        justify-content: center;
        text-align: center;
      }
    }
    .project-stats .nav {
      justify-content: center;
    }
  }

  // Switch to our version of GitLab logo.
  if dark_logo {
    // 404 logo.
    a[href="/"] > img[alt="GitLab Logo"] {
      height: 0 !important;
      width: 0 !important;
      padding: 210px 0 0 210px !important;
      background: url("data:image/svg+xml;base64,") no-repeat !important;
    }
    // Dark tanuki!
    .tanuki-logo {
      .tanuki-left-ear, .tanuki-left-cheek {
        fill: #212121 !important;
        stroke: var(--shadow);
      }
      .tanuki-right-ear, .tanuki-right-cheek {
        fill: #31363b !important;
        stroke: var(--shadow);
      }
      .tanuki-nose {
        fill: #303539 !important;
        stroke: var(--shadow);
      }
      .tanuki-left-eye {
        fill: #7289da !important;
      }
      .tanuki-right-eye {
        fill: #3daee9 !important;
      }
    }
  }

  // * Custom app background.
  body::after {
    position: fixed;
    z-index: -1;
    content: "";
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-position: center;
    background-repeat: bg_repeat;
    background-size: bg_size;
    filter: invert(bg_invert) bg_filter;
  }
  // ! Use literal css because stylus has its own url() function.
  @css {
    body::after {
      background: var(--background);
    }
  }

  /* Prevent horizontal scrollbar from showing up when the viewport is overflowed. */
  html {
    overflow-x: hidden;
  }
}
